<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Google Calendar Plugin">
    <Require feature="opensocial-0.8" />
 	 <UserPref name="code" display_name="Code Snippet from Jira Collector (jQuery)" datatype="string" required="true"/> 
  </ModulePrefs>
  <Content type="html">
  	<![CDATA[
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  		<script>
  			$(function() {
	  			var prefs = gadgets.prefs();
  			
  				prefs.code();
  			});	
  		</script>
  	]]>
  </Content>
   
</Module>